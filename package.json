    "antd": "^4.0.3",
    "axios": "^0.19.0",
    "cross-env": "^6.0.3",
    "dotenv-webpack": "^1.7.0",
    "express": "^4.17.1",
    "firebase": "^7.6.2",
    "formik": "^2.1.4",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-lazy-load-image-component": "^1.4.1",
    "react-notifications": "^1.4.3",
    "react-router-dom": "^5.1.2",
    "react-scroll-up": "^1.3.3",
    "serviceworker-webpack-plugin": "^1.0.1",
    "workbox-background-sync": "^5.1.1",
    "workbox-precaching": "^5.0.0",
    "yup": "^0.28.3"
  }
